						<foaf:Document rdf:about="http://www.bbc.co.uk/nature/21660528">
			<dc:title>Woolly rhino reveals Arctic Britain</dc:title>
			<dc:description>Scientists studying an exceptionally well-preserved woolly rhinoceros have revealed details of Britain's environment 42,000 years ago.</dc:description>
			<foaf:primaryTopic rdf:resource="/nature/species/Woolly_rhinoceros#species"/>
		</foaf:Document>
				<foaf:Document rdf:about="http://www.bbc.co.uk/nature/20497828">
			<dc:title>Prehistoric rhino reveals secrets</dc:title>
			<dc:description>The preserved body of a woolly rhinoceros reveals new insights into how this now extinct giant animal once lived.</dc:description>
			<foaf:primaryTopic rdf:resource="/nature/species/Woolly_rhinoceros#species"/>
		</foaf:Document>
				<foaf:Document rdf:about="http://www.bbc.co.uk/news/science-environment-14754317">
			<dc:title>'Oldest' woolly rhino discovered</dc:title>
			<dc:description>A woolly rhino fossil dug up on the Tibetan Plateau is believed to be the oldest specimen of its kind yet found. </dc:description>
			<foaf:primaryTopic rdf:resource="/nature/species/Woolly_rhinoceros#species"/>
		</foaf:Document>
